{
    "name": "nativescript-appurl-handler",
    "version": "1.0.0",
    "description": "Register custom URLs for your NativeScript app (IOS fix included)",
    "scripts": {
        "tsc": "cpy '**/*.d.ts' '../plugin' --parents --cwd=src && cp README.md plugin && tsc -skipLibCheck",
        "clean": "rimraf ./plugin/**/*.d.ts ./plugin/**/*.js ./plugin/**/*.js.map ./plugin/**/*.metadata.json plugin/node_modules plugin/package-lock.json",
        "build": "npm run tsc",
        "publish": "npm run build && lerna publish",
        "commitmsg": "commitlint -e $GIT_PARAMS"
    },
    "main": "urlhandler",
    "typings": "urlhandler.d.ts",
    "dependencies": {},
    "devDependencies": {
        
        "@commitlint/cli": "^8.3.5",
        "@types/jasmine": "3.5.10",
        "@typescript-eslint/eslint-plugin": "2.28.0",
        "@typescript-eslint/parser": "2.28.0",
        "cpy-cli": "^3.1.0",
        "eslint": "6.8.0",
        "husky": "^4.2.5",
        "lerna": "^3.20.2",
        "rimraf": "^3.0.2",
        "@nativescript/core": "~6.5.1",
        "tns-platform-declarations": "~6.5.1",
        "typescript": "3.8.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/farfromrefug/nativescript-appurl.git"
    },
    "nativescript": {
        "platforms": {
            "android": "3.4.0",
            "ios": "3.4.0"
        },
        "plugin": {
            "nan": "true",
            "pan": "true",
            "core3": "true",
            "category": "Processing"
        }
    },
    "keywords": [
        "NativeScript",
        "URL-Handler"
    ],
    "author": "Martin Guillong",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/farfromrefug/nativescript-appurl/issues"
    },
    "homepage": "https://github.com/farfromrefug/nativescript-appurl",
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    }
}
